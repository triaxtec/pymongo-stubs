from typing import Any

from pymongo.errors import InvalidOperation as InvalidOperation

class _WriteResult:
    def __init__(self, acknowledged: Any) -> None: ...
    @property
    def acknowledged(self): ...

class InsertOneResult(_WriteResult):
    def __init__(self, inserted_id: Any, acknowledged: Any) -> None: ...
    @property
    def inserted_id(self): ...

class InsertManyResult(_WriteResult):
    def __init__(self, inserted_ids: Any, acknowledged: Any) -> None: ...
    @property
    def inserted_ids(self): ...

class UpdateResult(_WriteResult):
    def __init__(self, raw_result: Any, acknowledged: Any) -> None: ...
    @property
    def raw_result(self): ...
    @property
    def matched_count(self): ...
    @property
    def modified_count(self): ...
    @property
    def upserted_id(self): ...

class DeleteResult(_WriteResult):
    def __init__(self, raw_result: Any, acknowledged: Any) -> None: ...
    @property
    def raw_result(self): ...
    @property
    def deleted_count(self): ...

class BulkWriteResult(_WriteResult):
    def __init__(self, bulk_api_result: Any, acknowledged: Any) -> None: ...
    @property
    def bulk_api_result(self): ...
    @property
    def inserted_count(self): ...
    @property
    def matched_count(self): ...
    @property
    def modified_count(self): ...
    @property
    def deleted_count(self): ...
    @property
    def upserted_count(self): ...
    @property
    def upserted_ids(self): ...
